{
    "compilerOptions": 
    {
        "jsx": "react",
        "target": "es6",
        "module": "CommonJS",
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
       
        "esModuleInterop": true,
        "sourceMap": true,
        "strictNullChecks": true,
       
        "lib": [
            "dom",
            "es2017",
            "es2015.proxy",
            "dom.iterable"
          ]
    }, 
    "ts-node": {
        "transpileOnly": true
    },
   
    
}
